{
  "extends": "tslint:recommended",
  "rules": {
    "arrow-parens": false,
    "callable-types": false,
    "no-console": false,
    "no-unused-expression": false,
    "max-line-length": [true, 180],
    "object-literal-sort-keys": false
  },
  "linterOptions": {
    "exclude": [
      "client/cypress/**/*.js"
    ]
  }
}
